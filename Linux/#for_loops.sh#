#!/bin/bash

fav_states=('virginia' 'florida' 'hawaii' 'california' 'nevada')


for state in ${fav_states[@]}
do 
if [ $state == 'hawaii' ] 
  then
    echo "Hawaii is the bist"
  else
    echo "I'm not a fan of hawaii"
fi
done


nums=$(echo {0..9})

for num in ${nums[@]}
do
  if [ $num = 3 ] || [ $num = 5 ] || [ $num = 7 ]
  then
   echo $num
  fi
done

ls_out=$(ls)

for x in ${ls_out[@]}
do 
  echo $x
done
